{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJs",
    "lib": ["es6"],
    "allowJs": true,
    "outDir": "build",
    "rootDirs": ["src", "prisma"],
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}
